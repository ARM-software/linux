	dp0: display@c00000 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "arm,mali-d32";
		reg = <0xc00000 0x20000>;
		interrupts = <0 168 4>;
		interrupt-names = "DPU";
		clocks = <&dpu_mclk>, <&dpu_aclk>;
		clock-names = "mclk", "pclk";

		pl0: pipeline@0 {
			clocks = <&fpgaosc2>, <&dpu_aclk>;
			clock-names = "pxclk", "aclk";
			reg = <0>;

			ports {
				#address-cells = <1>;
				#size-cells = <0>;

				port@0 {
					reg = <0>;
					dp0_pl0_out: endpoint {

					};
				};
			};
		};
	};
	aeu: aeu@c20000 {
		compatible = "arm,mali-aeu";
		reg = <0xc20000 0x20000>;
		interrupts = <0 168 4>;
		interrupt-names = "AEU";
		clocks = <&dpu_aclk>;
		clock-names = "aclk";
	};
