
/*
 * arm Ltd. Virtual encoder (2018)
 */

	vencoder1 {
		compatible = "drm,virtual-encoder";
		/* command_mode="true"; */
		status = "okay";
		display-timings {
			native-mode = <&timing0>;
			timing0: timing@0 {
				/* 640x480 framebuffer */
				clock-frequency = <23750000>;
				hactive = <640>;
				vactive = <480>;
				hfront-porch = <48>;
				hback-porch = <16>;
				hsync-len = <96>;
				vfront-porch = <33>;
				vback-porch = <9>;
				vsync-len = <3>;
			};

			timing1: timing@1 {
				/* 1280x720 framebuffer */
				clock-frequency = <74440000>;
				hactive = <1280>;
				vactive = <720>;
				hfront-porch = <56>;
				hback-porch = <192>;
				hsync-len = <136>;
				vfront-porch = <1>;
				vback-porch = <22>;
				vsync-len = <3>;
			};
		};

		port {
			vencoder_in1: endpoint {
			};
		};

	};

