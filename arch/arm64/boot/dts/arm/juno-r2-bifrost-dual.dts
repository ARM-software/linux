#include "juno-r2.dts"
#include "juno-bifrost.dtsi"

/ {
	gpu_fpga2: gpu@0x6e010000 {
		compatible = "arm,mali-midgard";
		reg = <0x0 0x6e010000 0x0 0x4000>;
		interrupts = <0 168 4>, <0 168 4>, <0 168 4>;
		interrupt-names = "JOB", "MMU", "GPU";
		system-coherency = <31>;
	};
};

&gpu_fpga {
	reg = <0x0 0x6e000000 0x0 0x4000>;
	system-coherency = <31>;
};
